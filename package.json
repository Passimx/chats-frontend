{
  "name": "PassimX",
  "description": "Open platform for messaging",
  "version": "1.3.4",
  "private": false,
  "type": "module",
  "license": "MIT LICENSE",
  "scripts": {
    "dev": "vite",
    "format": "prettier . --write",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky install",
    "dist:sha256": "cd dist && find . -type f -not -name \"*.map\" -print0 | sort -z | xargs -0 sha256sum | sed 's|^\\(.*\\)\\ \\./|\\1\\ |' > ../verify/dist.sha256",
    "verify": "cd verify && node verify.js"
  },
  "dependencies": {
    "@types/qrcode": "^1.5.6",
    "@yudiel/react-qr-scanner": "^2.4.1",
    "i18next": "^23.14.0",
    "md5": "^2.3.0",
    "moment": "^2.30.1",
    "music-metadata-browser": "^2.5.11",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-i18next": "^15.0.1",
    "react-icons": "^5.2.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.2",
    "ton-crypto": "^3.2.0",
    "tonweb": "^0.0.66",
    "vite-plugin-node-polyfills": "^0.24.0"
  },
  "devDependencies": {
    "@react-buddy/ide-toolbox": "^2.4.0",
    "@reduxjs/toolkit": "^2.3.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^13.1.2",
    "@types/md5": "^2.3.5",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.3.0",
    "@types/react-redux": "^7.1.33",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.7",
    "prettier": "3.3.2",
    "semantic-release": "^25.0.2",
    "typescript": "^5.2.2",
    "vite": "^7.3.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.--write": "prettier --write"
  },
  "engines": {
    "node": "20.x"
  }
}
